{
  "extends": "proste/reactTS.js",
  "plugins": ["jest"],
  "env": {
    "jest/globals": true
  },
  "parserOptions": {
    "project": "./tsconfig.json",
    "tsconfigRootDir": ".",
    "warnOnUnsupportedTypeScriptVersion": true
  },
  "overrides": [
    {
      "files": ["config/*", "scripts/*"],
      "parserOptions": {},
      "extends": "proste/javascript.js",
      "rules": {
        "no-console": 0
      }
    }
  ]
}