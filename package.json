{
  "name": "rwt",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "description": "An out of the box react webpack template,It is simpler, more convenient to customize and more modular on the basis of create-react-app.",
  "author": "xyhxx (603331487@qq.com)",
  "license": "MIT",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "eslint",
        "typescript",
        "postcss",
        "jest"
      ]
    }
  },
  "scripts": {
    "dev": "pnpm -F app dev",
    "build": "pnpm -F app build",
    "test": "pnpm -F app test",
    "test:watch": "pnpm -F app test:watch",
    "test:coverage": "pnpm -F app test:coverage",
    "e2e:run": "cypress run -b chrome",
    "e2e": "cypress open"
  },
  "devDependencies": {
    "@types/ip": "^1.1.0",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "cypress": "^12.5.1",
    "eslint": "^8.29.0",
    "eslint-config-proste": "^3.3.3",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "ip": "^1.1.8",
    "jest": "^29.3.1",
    "postcss": "^8.4.19",
    "stylelint": "^14.16.0",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^29.0.0",
    "typescript": "^4.9.4"
  }
}
